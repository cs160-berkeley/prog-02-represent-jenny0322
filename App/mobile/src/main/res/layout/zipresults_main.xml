<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ffffff"
    >
    <ImageView
        android:id= "@+id/avatar_dynamic"
        android:layout_width="90dp"
        android:layout_height="110dp"
        android:layout_marginTop="7dp"
        android:layout_marginLeft="10dp"


        />

    <TextView
        android:id = "@+id/name_dynamic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/avatar_dynamic"
        android:text = "Janet Bewley"
        android:layout_alignTop="@id/avatar_dynamic"
        android:layout_marginLeft="8dp"
        android:textSize="22dp"
        android:textStyle="bold"
        android:textColor="#000000"/>
    <TextView
        android:id="@+id/party_dynamic"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/name_dynamic"
        android:layout_alignLeft="@id/name_dynamic"

        android:text = "Democrat"
        android:textSize="16dp"

        android:textColor="#2853fc" />

    <TextView
        android:id="@+id/type_dynamic"
       android:layout_toRightOf="@id/party_dynamic"
        android:layout_below="@id/name_dynamic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text = "Senator"
        android:textSize = "16dp"
        android:textColor="#000000"
        />

    <ImageView
        android:id="@+id/emailheading_dynamic"
        android:layout_width="22dp"
        android:layout_height="20dp"
        android:background="@drawable/mail"
        android:layout_alignLeft="@id/name_dynamic"
        android:layout_toRightOf="@id/avatar_dynamic"
        android:layout_below="@id/party_dynamic"
        android:textColor="#000000"
        android:layout_marginTop="2dp"

        />

    <TextView
        android:id="@+id/email_dynamic"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/name_dynamic"
        android:layout_alignTop="@id/emailheading_dynamic"
        android:textSize="12dp"

        android:layout_marginLeft="2dp"
        android:textColor="#0B0080"
        android:layout_toRightOf="@id/emailheading_dynamic"
        android:text = "sen.feinstein@opencongress.org"/>

    <ImageView
        android:id="@+id/siteheading_dynamic"
        android:layout_width="22dp"
        android:layout_height="20dp"
        android:background="@drawable/mac"
        android:layout_alignLeft="@id/name_dynamic"
        android:layout_toRightOf="@id/avatar_dynamic"
        android:layout_below="@id/email_dynamic"
        android:textColor="#000000"
        android:layout_marginTop="2dp"

        />

    <TextView
        android:id="@+id/site_dynamic"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/email_dynamic"
        android:textColor="#0B0080"
        android:layout_toRightOf="@id/siteheading_dynamic"
        android:layout_alignTop="@id/siteheading_dynamic"
        android:layout_marginLeft="2dp"
        android:textSize="12dp"
        android:text = "http://legis.wisconsin.gov/senate/25/bewley"/>

    <FrameLayout
        android:id="@+id/tweet_dynamic"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:textSize = "15dp"
        android:layout_below="@id/site_dynamic"

        android:layout_marginTop="3dp"
        >



    </FrameLayout>
