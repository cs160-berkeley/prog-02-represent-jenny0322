<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="motionlogger.jennychen.com.represent.MainActivity"
    tools:showIn = "@layout/activity_results"
    android:background="#f1f1f1"
    >
    <View
        android:layout_width="match_parent"
        android:layout_height="156dp"
        android:id= "@+id/panel1"
        android:background="#ffffff" />

    <ImageView
        android:id= "@+id/avatar1"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/janet_bewley"

        />

    <TextView
        android:id = "@+id/name1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/avatar1"
        android:text = "Janet Bewley"
        android:layout_alignTop="@id/avatar1"
        android:layout_marginLeft="10dp"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textColor="#000000"/>
    <TextView
        android:id="@+id/party1"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/avatar1"
        android:layout_alignLeft="@id/avatar1"
        android:gravity="center"
        android:text = "Democrat"
        android:textSize="18dp"
        android:layout_marginTop="3dp"
        android:textColor="#2853fc" />

    <TextView
        android:id="@+id/type1"
        android:layout_alignLeft="@id/party1"
        android:gravity="center"
        android:layout_below="@id/party1"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text = "Senator"
        android:textSize = "18dp"
        android:textColor="#000000"
        android:layout_marginTop="2dp"/>
    <TextView
        android:id="@+id/email1heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email:"
        android:layout_alignLeft="@id/name1"
        android:layout_toRightOf="@id/avatar1"
        android:layout_below="@id/name1"
        android:textColor="#000000"
        android:layout_marginTop="1dp"
        android:textSize="12dp"
        />

    <TextView
        android:id="@+id/email1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#0B0080"
        android:textSize="12dp"
        android:layout_marginLeft="2dp"
        android:text = "sen.bewley@legis.wi.gov"
        android:layout_toRightOf="@id/email1heading"
        android:layout_alignBottom="@+id/email1heading"
         />

    <TextView
        android:id="@+id/site1heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Site:"
        android:layout_alignLeft="@id/name1"
        android:layout_toRightOf="@id/avatar1"
        android:layout_below="@id/email1heading"
        android:textColor="#000000"
        android:layout_marginTop="2dp"
        android:textSize="12dp"
        />

    <TextView
        android:id="@+id/site1"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/email1"
        android:textColor="#0B0080"
        android:layout_toRightOf="@id/site1heading"
        android:layout_marginLeft="2dp"
        android:layout_alignTop="@id/site1heading"
        android:textSize="12dp"
        android:text = "http://legis.wisconsin.gov/senate/25/bewley"/>

    <Button
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignRight="@id/panel1"
        android:layout_marginRight="5dp"
        android:layout_marginTop="67dp"
        android:background = "@drawable/next"
        style="?android:attr/borderlessButtonStyle"
        android:onClick = "viewDetail1"
        />
    <TextView
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:id="@+id/twitter1"
        android:text="Janet Bewley @janetbewey4wi 2h"
        android:textSize="11dp"
        android:layout_below="@id/site1"
        android:layout_alignLeft="@id/email1heading"
        />
    <TextView
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:id="@+id/twitter_detail1"
        android:text = "Congratulations to our new chariwomen, Marth Laning !! Follow her @laningforwi"
        android:layout_below="@id/twitter1"
        android:layout_alignLeft="@id/twitter1"
        android:textSize="12dp"
        android:textColor="#000000"
        />


    <View
        android:layout_width="match_parent"
        android:layout_height="156dp"
        android:id= "@+id/panel2"
        android:background="#ffffff"
        android:layout_below="@id/panel1"
        android:layout_marginTop="10dp"
        />

    <ImageView
        android:id= "@+id/avatar2"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/rob_cowles"
        android:layout_below ="@id/panel1"
        android:layout_marginTop = "20dp"

        />

    <TextView
        android:id = "@+id/name2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/avatar2"
        android:text = "Robert Cowles"
        android:layout_alignTop="@id/avatar2"
        android:layout_marginLeft="10dp"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textColor="#000000"/>
    <TextView
        android:id="@+id/party2"
        android:layout_width="100dp"
        android:gravity="center"
        android:layout_height="wrap_content"
        android:layout_below="@id/avatar2"
        android:layout_alignLeft="@id/avatar2"
        android:text = "Republican"
        android:textSize="18dp"
        android:layout_marginTop="3dp"
        android:textColor="#d10101" />

    <TextView
        android:id="@+id/type2"
        android:layout_alignLeft="@id/party2"
        android:layout_below="@id/party2"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text = "Senator"
        android:textSize = "18dp"
        android:textColor="#000000"
        android:layout_marginTop="3dp"/>

    <TextView
        android:id="@+id/email2heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email:"
        android:layout_alignLeft="@id/name2"
        android:layout_toRightOf="@id/avatar2"
        android:layout_below="@id/name2"
        android:textColor="#000000"
        android:layout_marginTop="1dp"
        android:textSize="12dp"
        />

    <TextView
        android:id="@+id/email2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/name2"
        android:textColor="#0B0080"
        android:layout_alignTop="@id/email2heading"
        android:layout_marginLeft="2dp"
        android:layout_toRightOf="@id/email2heading"
        android:textSize="12dp"
        android:text = "sen.cowles@legis.wi.gov"/>

    <TextView
        android:id="@+id/site2heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Site:"
        android:layout_alignLeft="@id/name2"
        android:layout_toRightOf="@id/avatar2"
        android:layout_below="@id/email2heading"
        android:textColor="#000000"
        android:layout_marginTop="1dp"
        android:textSize="12dp"
        />

    <TextView
        android:id="@+id/site2"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/email2"
        android:textColor="#0B0080"
        android:layout_toRightOf="@id/site2heading"
        android:layout_marginLeft="1dp"

        android:textSize="12dp"
        android:text = "http://legis.wisconsin.gov/senate/cowles/Pages/default.aspx"
        />

    <Button
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignRight="@id/panel2"
        android:layout_marginRight="5dp"
        android:layout_marginTop="230dp"
        android:background = "@drawable/next"
        style="?android:attr/borderlessButtonStyle"
        android:onClick = "viewDetail2"
        />
    <TextView
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:id="@+id/twitter2"
        android:text="Rob Cowles @SenRobCowles 2h"
        android:textSize="11dp"

        android:layout_below="@id/site2"
        android:layout_alignLeft="@id/email2heading"/>

    <TextView
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:id="@+id/twitter_detail2"
        android:text = "Retweeted @SenRobCowles meets with #citystadiumauto motive students"
        android:layout_below="@id/twitter2"
        android:layout_alignLeft="@id/twitter2"
        android:textSize="12dp"
        android:textColor="#000000"
        />

    <View
        android:layout_width="match_parent"
        android:layout_height="156dp"
        android:id= "@+id/panel3"
        android:background="#ffffff"
        android:layout_below="@id/panel2"
        android:layout_marginTop="10dp"
        />
    <ImageView
        android:id= "@+id/avatar3"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/scott_allen"
        android:layout_below ="@id/panel2"
        android:layout_marginTop = "20dp"

        />

    <TextView
        android:id = "@+id/name3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/avatar3"
        android:text = "Scott Allen"
        android:layout_alignTop="@id/avatar3"
        android:layout_marginLeft="10dp"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textColor="#000000"/>

    <TextView
        android:id="@+id/party3"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/avatar3"
        android:layout_alignLeft="@id/avatar3"
        android:gravity="center"
        android:text = "Republican"
        android:textSize="18dp"
        android:layout_marginTop="3dp"
        android:textColor="#d10101" />

    <TextView
        android:id="@+id/type3"
        android:layout_alignLeft="@id/party3"
        android:layout_below="@id/party3"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text = "Rep."
        android:textSize = "18dp"
        android:textColor="#000000"
        android:layout_marginTop="3dp"/>

    <TextView
        android:id="@+id/email3heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email:"
        android:layout_alignLeft="@id/name3"
        android:layout_toRightOf="@id/avatar3"
        android:layout_below="@id/name3"
        android:textColor="#000000"
        android:layout_marginTop="1dp"
        android:textSize="12dp"
        />

    <TextView
        android:id="@+id/email3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/name3"
        android:textColor="#0B0080"
        android:layout_marginTop="1dp"
        android:layout_toRightOf="@id/email3heading"
        android:layout_marginLeft="2dp"
        android:text = "rep.allen@legis.wi.gov"
        android:singleLine="false"
        android:textSize="12dp"/>
    <TextView
        android:id="@+id/site3heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Site:"
        android:layout_alignLeft="@id/name3"
        android:layout_toRightOf="@id/avatar3"
        android:layout_below="@id/email3heading"
        android:textColor="#000000"
        android:layout_marginTop="2dp"
        android:textSize="12dp"
        />

    <TextView
        android:id="@+id/site3"
        android:layout_width="155dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/email3"
        android:textColor="#0B0080"
        android:layout_toRightOf="@id/site3heading"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:textSize="12dp"
        android:text = "http://legis.wisconsin.gov/assembly/97/allen"/>

    <Button
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignRight="@id/panel3"
        android:layout_marginRight="5dp"
        android:layout_marginTop="395dp"
        android:background = "@drawable/next"
        style="?android:attr/borderlessButtonStyle"
        android:onClick = "viewDetail3"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/twitter3"
        android:text = "Scott Allen @scott_allen 8h"
        android:layout_below="@id/site3"
        android:layout_alignLeft="@id/site3heading"
        android:textSize="11dp"
        android:layout_marginTop="3dp"/>
    <TextView
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:id="@+id/twitter_detail3"
        android:text = "8 Smart Bathroom Design Ideas. These are great! @hgtv"
        android:layout_below="@id/twitter3"
        android:layout_alignLeft="@id/twitter3"
        android:textSize="12dp"
        android:textColor="#000000"
        />






</RelativeLayout>
